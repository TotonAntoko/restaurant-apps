{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "accessor-pairs": "off",
    "no-undef": "off",
    "no-prototype-builtins": "off"
  },
  "plugins": [
    "jasmine"
  ]
}